<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="ColorFieldView">
        <attr name="defaultCellColor" format="color" />
        <attr name="lineColor" format="color" />
        <attr name="palette" format="reference" />
        <attr name="countX" format="integer" />
        <attr name="countY" format="integer" />
        <attr name="cellPadding" format="dimension" />
        <attr name="strokeWidth" format="integer" />
        <attr name="normaliseForLowestEdge" format="boolean" />
        <attr name="cellMargin" format="dimension" />
    </declare-styleable>
    
    <declare-styleable name="ColorFieldViewEditBoard">
        <attr name="preferedCellWidth" format="dimension"/>
        <attr name="preferedCellHeight" format="dimension"/>
        <attr name="alignCellsToCenter" format="boolean"/>
    </declare-styleable>
</resources>